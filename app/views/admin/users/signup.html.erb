<h1>Signup</h1>
<% @user.password = @user.password_confirmation = nil %> 
<% form_for :user, @user, :url => {:controller => 'admin/users', :action => 'register'}, :html => { :multipart => true } do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :login %><br />
    <%= f.text_field :login %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation %>
  </p>
  <p>
    (optional photo)<br />
    <%= f.file_field :photo %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>
